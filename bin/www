#!/usr/bin/env node

/**
 * Created by Jordan on 2/20/2015.
 */

var parameters  = require('../parameters'),
    models      = require('../models'),
    winston     = require('winston'),
    app         = require('../index');

models.sequelize.sync().then(function () {
    app.listen(parameters.server.port, function () {
        console.log(parameters.server.port);
        winston.log('Server listening on', parameters.server.port);
    });
});
